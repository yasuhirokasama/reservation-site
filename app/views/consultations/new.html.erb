<%= form_with model: @consultation do |form| %>
  <div>
    <label>名前</label>
    <%= form.text_field :name %>
  </div>
  <div>
    <label>携帯番号</label>
    <%= form.text_field :cell_number %>
  </div>
  <div>
    <label>電話番号</label>
    <%= form.text_field :tel_number %>
  </div>
  <div>
    <label>email</label>
    <%= form.text_field :email %>
  </div>
  <div>
    <label>生年月日</label>
    <%= form.date_field :birthday %>
  </div>
  <div>
    <label>性別</label>
    <%= form.number_field :sex %>
  </div>
  <div>
    <label>診断種別</label>
    <%= form.number_field :visit_type %>
  </div>
  <div>
    <label>予約日時</label>
    <%= form.datetime_field :visit_at %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
